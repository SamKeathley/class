<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Units</title>
  </head>
  <body>
    <h1>Units</h1>

    <% for (var i=0, x=units.length; i<x; i++) { %>
      <img src="<%= units[i].imageUrl %>" />
      <p>Name: <%= units[i].name %></p>
      <p>Description: <%= units[i].description %></p>
      <p>Mineral Cost: <%= units[i].mineral %></p>
    <% } %>
  </body>
</html>
